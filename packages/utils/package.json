{
  "name": "@mindease/utils",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "description": "Shared utility functions for MindEase",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@mindease/eslint-config": "file:../eslint-config",
    "@mindease/typescript-config": "file:../typescript-config",
    "@mindease/vitest-preset": "file:../vitest-preset",
    "@types/node": "20.19.x",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "typescript": "5.8.3",
    "vitest": "^3.1.3"
  }
}
